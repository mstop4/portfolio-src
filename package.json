  "license": "ISC",
  "homepage": "https://jonathanlam.ca",
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/preset-env": "^7.1.5",
    "babel-preset-minify": "^0.5.0",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "css-concat": "0.0.4",
    "uglifycss": "0.0.29"
  }
}
